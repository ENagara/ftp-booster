{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --config app.dev.json --tunnel",
    "build:web:prod": "expo build:web",
    "build:web:dev": "expo build:web --config app.dev.json",
    "deploy:web:prod": "firebase deploy",
    "deploy:web:dev": "firebase hosting:channel:deploy DEV",
    "_sentry:upload": "sentryにsourcemapsをアップロード",
    "sentry:upload:prod": "cross-conf-env sentry-cli releases files npm_package_config_version upload-sourcemaps --ext js --ext map web-build",
    "sentry:upload:dev": "cross-conf-env sentry-cli releases files DEV upload-sourcemaps --ext js --ext map web-build",
    "_release": "ビルドからデプロイまで実行",
    "release:prod": "run-s build:web:prod deploy:web:prod sentry:upload:prod",
    "release:dev": "run-s build:web:dev  deploy:web:dev sentry:upload:dev",
    "_firebase:saveindex": "firestoreで作成したindexをfirestore.indexes.jsonに反映",
    "firebase:saveindex": "firebase firestore:indexes > firestore.indexes.json"
  },
  "config": {
    "_version": "本番のバージョン設定 app.jsonのversionも合わせる",
    "version": "1.0.0"
  },
  "dependencies": {
    "@eva-design/eva": "^2.0.0",
    "@react-native-community/datetimepicker": "3.0.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.7",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "@sentry/browser": "^6.2.1",
    "@ui-kitten/components": "^5.0.0",
    "chart.js": "^2.9.4",
    "expo": "~40.0.0",
